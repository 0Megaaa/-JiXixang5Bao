<?xml version="1.0" encoding="UTF-8" ?>  
<!DOCTYPE mapper PUBLIC "-//ibatis.apache.org//DTD Mapper 3.0//EN" 
	"http://ibatis.apache.org/dtd/ibatis-3-mapper.dtd">
<mapper namespace="com.soft.mapper.PayChannelMapper">


	<select id="findAotuChannel"  resultType="long">
	select SUM(money) from tbcar  where paychannel = 1
	</select>

	<select id="findPhoneChannel" resultType="long" >
		select SUM(money) from tbcar where paychannel = 2
	</select>
	
 

 
</mapper>